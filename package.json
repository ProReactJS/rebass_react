{
  "name": "rebass",
  "version": "1.0.0",
  "description": "React components for Basscss",
  "author": "Brent Jackson",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "brfs": "^1.3.0",
    "browserify": "^8.0.3",
    "envify": "^3.2.0",
    "http-server": "^0.7.4",
    "jsx-loader": "^0.12.2",
    "lodash": "^3.1.0",
    "node-jsx": "^0.12.4",
    "parallelshell": "^1.0.4",
    "react": "^0.12.2",
    "react-tools": "^0.12.2",
    "reactify": "^1.0.0",
    "uglify": "^0.1.1",
    "watch": "^0.14.0",
    "watchify": "^2.3.0"
  },
  "keywords": [
    "React",
    "Components",
    "react-component",
    "UI",
    "Basscss"
  ],
  "scripts": {
    "watchify": "watchify src/app.js -o js/app.js",
    "uglify": "uglify js/app.js -o js/app.min.js",
    "watch:uglify": "watch 'npm run uglify' ./js",
    "build:js": "NODE_ENV=production browserify src/app.js -o js/app.js",
    "build:html": "node src/build",
    "watch:html": "watch 'npm run build:html' ./src",
    "server": "http-server -p 8000 ",
    "start": "parallelshell 'npm run watchify' 'npm run watch:uglify' 'npm run watch:html' 'npm run server'"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  }
}
