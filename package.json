{
  "name": "rebass",
  "version": "0.1.0",
  "description": "React UI components for Basscss",
  "author": "Brent Jackson",
  "license": "MIT",
  "scripts": {
    "babel": "babel src --out-dir .",
    "babel:watch": "babel src -w --out-dir .",
    "docs": "webpack --progress --colors --config webpack.config.js",
    "docs:dev": "webpack-dev-server --progress --colors --config webpack.config.js",
    "start": "npm run babel:watch & npm run docs:dev"
  },
  "dependencies": {
    "basscss": "^7.0.0",
    "classnames": "^1.1.4",
    "colorbass": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.5.6",
    "babel-core": "^5.5.6",
    "babel-loader": "^5.1.4",
    "basscss.github.io": "github:basscss/basscss.github.io",
    "css-loader": "^0.14.5",
    "cssnext-loader": "^1.0.1",
    "highlight.js": "^8.4.0",
    "jsx-loader": "^0.12.2",
    "node-libs-browser": "^0.5.2",
    "react": "^0.13.3",
    "rebass-example": "^1.1.1",
    "static-site-generator-webpack-plugin": "^1.0.0",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "keywords": [
    "basscss",
    "components",
    "react",
    "react-component",
    "ui"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jxnblk/rebass.git"
  },
  "bugs": {
    "url": "https://github.com/jxnblk/rebass/issues"
